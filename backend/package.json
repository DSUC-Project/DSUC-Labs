{
  "name": "dsuc-backend",
  "version": "0.1.0",
  "description": "Backend for DSUC Lab - Node.js + Express + Supabase",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon --watch src --exec \"node -r dotenv/config -r ts-node/register src/index.ts\"",
    "build": "tsc -p tsconfig.json",
    "migrate": "node ./dist/migrate.js",
    "seed": "ts-node scripts/seed.ts"
  },
  "author": "DSUC",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/cors": "^2.8.12",
    "@types/multer": "^1.4.7",
    "@types/dotenv": "^8.2.0"
  }
}
